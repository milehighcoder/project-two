<link rel="stylesheet" href="css/style1.css">
<div class="logo">
    <img src="assets/img/when-we-clean-logo.png" alt="">
</div>
<div id="join-portal-wrapper" class="row columns">
    <div class="translucent-form-overlay">
        <form>
            <h3 class="center">Join Portal</h3>
            <br>
            <div class="sign-in-form">
                <label class="form-label" for="sign-up-form-first-name"><p>First Name</p></label>
                <input type="text" class="sign-in-form-username form-input" id="sign-up-form-first-name"
                name="First Name"
                required>
                <label class="form-label" for="sign-up-form-last-name"><p>Last Name</p></label>
                <input type="text" class="sign-in-form-username form-input" id="sign-up-form-last-name" name="Last Name"
                required>
                <label class="form-label" for="sign-up-form-phone"><p>Phone</p></label>
                <input type="text" class="sign-in-form-username form-input" id="sign-up-form-phone" name="Phone Number"
                required>
                <label class="form-label" for="sign-up-form-email"><p>Email</p></label>
                <input type="text" class="sign-in-form-username form-input" id="sign-up-form-email"
                name="Email"
                required>
                <label class="form-label" for="sign-up-form-password"><p>Password</p></label>
                <input type="password" class="sign-in-form-password form-input" id="sign-up-form-password" name="Password"
                required>
                <label class="form-label" for="sign-up-form-confirm-password"><p>Confirm Password</p></label>
                <input type="password" class="sign-in-form-password form-input" id="sign-up-form-confirm-password" name="Confirm Password"
                required>
                <input class="form-label" type="checkbox" id="sign-up-form-is-manager">
                <label for="sign-up-form-is-manager"><p2>Check if you are a manager.</p2></label>
                <button id="sign-up-form-register-btn" type="submit" class="sign-in-form-button">Create Account</button>
            </div>
            </br>
        </form>
    </div>
</div>
<script src="js/register.js" type="text/javascript"></script>;